CREATE TABLE HEATER (ID VARCHAR(255) NOT NULL, CONSO INTEGER, NAME VARCHAR(255), HOME_ID VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE HOME (ID VARCHAR(255) NOT NULL, NAME VARCHAR(255), NOMBREPIECE INTEGER, TAILLE INTEGER, PERSON_ID VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE ELECTRONICDEVICE (ID VARCHAR(255) NOT NULL, CONSO INTEGER, NAME VARCHAR(255), HOME_ID VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE PERSON (ID VARCHAR(255) NOT NULL, MAIL VARCHAR(255) UNIQUE, NOM VARCHAR(255), PRENOM VARCHAR(255), PRIMARY KEY (ID))
ALTER TABLE HEATER ADD CONSTRAINT FK_HEATER_HOME_ID FOREIGN KEY (HOME_ID) REFERENCES HOME (ID)
ALTER TABLE HOME ADD CONSTRAINT FK_HOME_PERSON_ID FOREIGN KEY (PERSON_ID) REFERENCES PERSON (ID)
ALTER TABLE ELECTRONICDEVICE ADD CONSTRAINT FK_ELECTRONICDEVICE_HOME_ID FOREIGN KEY (HOME_ID) REFERENCES HOME (ID)
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(38), PRIMARY KEY (SEQ_NAME))
